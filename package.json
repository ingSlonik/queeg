{
  "name": "queeg",
  "version": "1.1.2",
  "description": "Make from your the best usable websites desktop application customized just for you.",
  "main": "lib/index.js",
  "scripts": {
    "compile": "tsc",
    "electron": "electron .",
    "start": "npm run compile && npm run electron",
    "build:win": "electron-packager . app --platform win32 --arch x64 --out dist/ --icon=img/icon.ico",
    "build:linux": "electron-packager . app --platform linux --arch x64 --out dist/",
    "build:mac": "electron-packager . app --platform darwin --arch x64 --out dist/"
  },
  "license": "MIT",
  "keywords": [
    "easy&lite",
    "website",
    "desktop application"
  ],
  "author": "Filip Paul≈Ø <ing.fenix@seznam.cz>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ingSlonik/queeg.git"
  },
  "bugs": {
    "url": "https://github.com/ingSlonik/queeg/issues"
  },
  "devDependencies": {
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "electron": "^9.2.0",
    "electron-packager": "^15.0.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "easy-db-node": "^0.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
